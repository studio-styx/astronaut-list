    "fast-glob": "3.3.3",
    "node-cache": "^5.1.2",
    "rou3": "0.5.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "prisma": "6.1.0",
    "typescript": "5.7.2"
  },
  "imports": {
    "#database": [
      "./build/database/index.js"
    ],
    "#base": [
      "./build/discord/base/index.js"
